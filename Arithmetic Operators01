// Take a input form user and add those two numbers 

    #include<stdio.h>

    int main()
    {
        int a,b;
        printf("Enter an value for a :");
        scanf("%d",&a);
        printf("Enter an value for b :");
        scanf("%d",&b);
        
        int sum=a+b;
        printf("Addition of a and b is : %d\n",sum);

}
