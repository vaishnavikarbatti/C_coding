    #include<stdio.h>

        void swap()
        {
            int a;
            int b;
            printf("Enter an a value :");
            scanf("%d",&a);
            printf("Enter an b value :");
            scanf("%d",&b);

            int temp;
            temp=a;
            a=b;
            b=temp;

            printf("After done swapping a = %d , b = %d", a, b);
        }

        int main()
        {
            swap(34,78);
        }

        output:-

        Enter an a value :10
Enter an b value :20
After done swapping a = 20 , b = 10
        
