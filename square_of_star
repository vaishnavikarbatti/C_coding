#include<stdio.h>

    int main()
    {
        int m,n;
        printf("Enter no of rows :");
        scanf("%d",&n);
        printf("Enter no of columns :");
        scanf("%d",&m);

        for(int i=1;i<=m;i++)
        {
            for(int i=1;i<=n;i++)
            {
                printf("*");
            }
            printf("\n");
        }
    }
output:-
Enter no of rows    :4
Enter no of columns :4
****
****
****
****
