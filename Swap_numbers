#include<stdio.h>
    int main()
    {
        int temp;
        int a;
        int b;
        
        printf("Enter an a value :");
        scanf("%d",&a);
        printf("Enter an b value :");
        scanf("%d",&b);

        temp = a;
        a = b;
        b = temp;
        printf("\n");
        printf("After a = %d , b = %d\n",a,b);
        
    }

output:-

Enter an a value :3
Enter an b value :4

After a = 4 , b = 3
