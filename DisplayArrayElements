#include <stdio.h>

int main() {
    int arr[5];

    // Input
    printf("Enter 5 elements:\n");
    for (int i = 0; i < 5; i++) {
        scanf("%d", &arr[i]);
    }

    // Output
    printf("Array elements are:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}

output:-

Enter 5 elements:
10 20 30 40 50
Array elements are:
10 20 30 40 50
