#include <stdio.h>

void change(int x[5]) {
    x[3] = 90; 
}

int main() {
    int arr[5] = {4, 6, 7, 8, 6};

    printf("Original array:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }

    change(arr); 

    printf("\nModified array:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}

output:-
Original array:
4 6 7 8 6 
Modified array:
4 6 7 90 6
