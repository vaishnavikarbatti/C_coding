

 #include<stdio.h>
    void number(int num)
    {
        if(num==0)
        return;
        number(num-1);
        printf("%d\n",num);
    }

    int main()
    {
        int num;
        printf("Enter an values :");
        scanf("%d",&num);
        number(num);
        return 0;

    }


output:-
Enter an values :3
1
2
3
